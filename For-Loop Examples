#include <stdio.h>
#include <stdlib.h>

int main()
{

    /*int i = 1;

    while(i <= 5)
    {
        printf("%d\n", i);
        i++;
    }


    // This For-Loop is doing the same as the above while-loop
    int i;

    for(i = 1; i <= 5; i++)
    {
        printf("%d\n", i);
