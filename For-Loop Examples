    */

    int luckyNums[] = {4, 8, 15, 16, 23, 42};
    int i;

    // This for loop cycles through the luckyNums array according the position, provided by int 'i'
    for(i = 0; i < 6; i++)
    {
        printf("%d\n", luckyNums[i]);
    }

    return 0;
}
